{
  "name": "full-stack-template",
  "version": "1.0.0",
  "scripts": {
    "test": "echo Test Succeeded && exit 0",
    "backend:dev": "npm run --workspace=backend dev",
    "frontend:dev": "npm run --workspace=frontend dev",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "build": "npm run --workspace=backend build && npm run --workspace=frontend build",
    "start": "npm run --workspace=backend start && npm run --workspace=frontend start",
    "deploy": "npm run build && npm run start"
  },
  "workspaces": [
    "frontend",
    "backend",
    "common"
  ],
