{
  "name": "full-stack-template",
  "private": true,
  "scripts": {
    "build": "npm run turbo build",
    "dev": "dotenv -e .dev.env -e .public.env -e .secret.env -- npm run turbo dev",
    "lint": "npm run turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "serve": "dotenv -e .public.env -e .secret.env -- npm run turbo serve",
    "deploy": "npm run build && npm run serve",
    "check-types": "npm run turbo check-types",
    "turbo": "tsx scripts/turboRun.ts",
    "generate": "tsx scripts/generateProject.ts"
  },
  "devDependencies": {
    "@dotenv-run/cli": "^1.3.6",
    "@eslint-react/eslint-plugin": "^2.3.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "esbuild": "^0.27.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*/backend",
    "apps/*/frontend",
    "packages/*"
  ],
  "dependencies": {
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint-plugin-bsd": "^1.0.0",
    "eslint-plugin-react": "^7.37.5",
    "express": "^5.1.0",
    "tsx": "^4.20.6"
  }
}
