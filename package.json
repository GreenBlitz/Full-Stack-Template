    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "concurrently": "^9.2.1",
    "esbuild": "0.25.11",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
